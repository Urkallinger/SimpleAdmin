{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "eslint auto fix",
      "type": "shell",
      "command": "yarn",
      "args": [
        "eslint",
        "--ext",
        ".js",
        "--ignore-pattern",
        "\"src/resources/**\"",
        "--fix",
        "\"src/**\""
      ]
    }
  ]
}
